{%- if 'Actifs ayant un emploi' in TACT-%}
    {{'Tu travailles'}}
    {%- if 'complet' in TP -%}
        {{'à plein temps'|wspace}}
    {%- elif 'partiel' in TP-%}
        {{'à temps partiel'|wspace}}
    {%- endif -%}
    {%- if 'Non' not in STATR -%}
        {{'comme salarié'|wspace}}
    {%- else -%}
        {{'comme non salarié'|wspace}}
    {%- endif -%}
    {{'dans'|wspace}}
    {%- if (
        NA17.startswith('Activités') or
        NA17.startswith('Autres') or
        NA17.startswith('Transports')
        ) -%}
        {{'les'|wspace + NA17|desentence|wspace|decontext}}
    {%- elif (
        NA17.startswith('Commerce')
        ) -%}
        {{"le"|wspace + NA17|desentence|wspace}}
    {%- elif (
        NA17.startswith('Administration') or
        NA17.startswith('Agriculture') or
        NA17.startswith('Hébergement') or
        NA17.startswith('Industries') or
        NA17.startswith('Information')
        ) -%}
        {{"l'"|wspace + NA17|desentence}}
    {%- else -%}
        {{'la'|wspace + NA17|desentence|wspace}}
    {%- endif -%}
