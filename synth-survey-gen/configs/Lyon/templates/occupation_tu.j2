    {{'Tu es au chômage'}}
{%- elif 'Retraités' in TACT -%}
    {{'Tu es à la retraite'}}
{%- elif 'étudiants' in TACT -%}
    {{'Tu es étudiant'}}
{%- elif TACT == 'Femmes ou hommes au foyer' -%}
    {%- if SEXE == 'Hommes' -%}
        {{'Tu es un homme au foyer'}}
    {%- else -%}
        {{'Tu es une femme au foyer'}}
    {%- endif -%}
{%- else -%}
    {{'Tu ne travailles pas en ce moment'}}
